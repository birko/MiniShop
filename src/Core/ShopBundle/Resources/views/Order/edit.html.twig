{% extends '::admin.html.twig' %}
{% block body %}
<h1>{%trans%}Order edit{%endtrans%}</h1>
<form action="{{ path('order_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
  <fieldset>
    {{ form_widget(edit_form) }}
    <div class="form-actions">
        <button type="submit" class="btn btn-success"> <i class="icon-ok"></i> {%trans%}Edit{%endtrans%}</button>
    </div>
  </fieldset>  
</form>
<hr />
{% endblock %}