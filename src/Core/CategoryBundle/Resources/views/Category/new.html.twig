{% extends '::admin.html.twig' %}
{% block body %}
<h1>{% trans %}Category creation{% endtrans %}</h1>

<form action="{{ path('category_create', {'menu': menu, 'parent': parent}) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-success"><i class="icon-ok"></i> {% trans %}Create{% endtrans %}</button>
    </p>
</form>
{% endblock %}
